<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx" width="100%" height="55">
	
	<fx:Script>
		<![CDATA[
			import flash.net.URLRequest;
			import flash.net.navigateToURL;
			
			private function browseToHomepage():void
			{
				var url:URLRequest = new URLRequest("http://www.coldfusiondocs.com");
				navigateToURL(url, "_blank");
			}
		]]>
	</fx:Script>
	
	<s:Rect top="0" right="0" left="0" bottom="0">
		<s:fill>
			<s:SolidColor color="0x003399" />
		</s:fill>
	</s:Rect>

	<s:HGroup width="100%" height="55" paddingLeft="10" paddingTop="10">   
		<s:Label text="coldfusiondocs.com" fontSize="34" fontWeight="bold" 
				  fontFamily="Trebuchet MS" color="#ffffff"  click="browseToHomepage()" width="750" />  	
	</s:HGroup>
</s:Group>
