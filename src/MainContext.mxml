<?xml version="1.0" encoding="utf-8"?>
<fx:Object 
	xmlns="http://www.spicefactory.org/parsley"
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:module="com.adobe.cairngorm.module.*"
	xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:controller="com.adobe.ps.mobius.audit.controller.*">
	<fx:Script>
		<![CDATA[
			import com.coldfusiondocs.commands.LoadCFMLDataCommand;
			import com.coldfusiondocs.controller.MainController;
			import com.coldfusiondocs.model.AppData;
		]]>
	</fx:Script>
	
	<fx:Declarations>
		
		<s:HTTPService
			url="http://assets.coldfusiondocs.com/xml/cfmldoc.xml" 
			id="adobeCFMLService" 
			resultFormat="e4x" 
			showBusyCursor="true" />
		
		<s:HTTPService
			url="http://assets.coldfusiondocs.com/xml/cfmlrailo.xml" 
			id="railoCFMLService" 
			resultFormat="e4x" 
			showBusyCursor="true" />
		
		<s:WebService 
			wsdl="http://coldfusiondocs.com/services/linkmanager.cfc?wsdl" 
			id="linkService" 
			showBusyCursor="true" />
		
		<Object type="{MainController}" id="mainController"/>
		
		<Object type="{AppData}"/>
		
		<DynamicCommand type="{LoadCFMLDataCommand}"/>
		
	</fx:Declarations>
	
</fx:Object>
